{
  "name": "db-logger-microservices",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "commit": "cz",
    "build": "rm -rf .build && nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "npx eslint \"src/**/*.ts\"",
    "lint:fix": "yarn lint --fix",
    "watch": "nest start --debug 0.0.0.0:9229 --watch",
    "dev": "nest start",
    "prepare": "husky install",
    "start:prod": "node .build/server"
  },
  "dependencies": {
    "@influxdata/influxdb-client": "^1.29.0",
    "@influxdata/influxdb-client-apis": "^1.30.0",
    "@nestjs/common": "^8.4.3",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.4.3",
    "@nestjs/microservices": "^8.4.4",
    "@nestjs/platform-express": "^8.4.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.2",
    "env-var": "^7.3.0",
    "nats": "^2.7.1",
    "pino": "^8.6.1",
    "pino-http": "^8.2.0",
    "pino-pretty": "^9.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.4",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^8.4.3",
    "@types/express": "^4.17.14",
    "@types/jest": "27.4.1",
    "@types/node": "^18.7.23",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "jest": "^27.2.5",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.2.4",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
